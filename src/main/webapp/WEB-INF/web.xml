<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         version="2.5"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name>Archetype Created Web Application</display-name>


  <servlet>
    <servlet-name>registration</servlet-name>
    <servlet-class>servlets.javaClasses.Registration</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>registration</servlet-name>
    <url-pattern>/registration</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>home</servlet-name>
    <servlet-class>servlets.javaClasses.Home</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>home</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>

  <servlet>
  <servlet-name>login</servlet-name>
  <servlet-class>servlets.javaClasses.Login</servlet-class>
</servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>enter</servlet-name>
    <servlet-class>servlets.javaClasses.Enter</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>enter</servlet-name>
    <url-pattern>/enter</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>updateNote</servlet-name>
    <servlet-class>servlets.javaClasses.UpdateNote</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>updateNote</servlet-name>
    <url-pattern>/updateNote</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>createNote</servlet-name>
    <servlet-class>servlets.javaClasses.CreateNote</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>createNote</servlet-name>
    <url-pattern>/createNote</url-pattern>
  </servlet-mapping>
</web-app>
